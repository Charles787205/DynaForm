<div class="input-block title" draggable="true" id="<%= query.id || 'none' %>" >
	<div class="block-container">
		<div
			show-pl-click="true"
			contenteditable="true"
			placeholder="<%=query.placeholder || 'heading' %>"
			data-type="heading"
			required
			spellcheck="false"
			data-name="<%= query.name || 'none' %>_<%= query.id || "none" %>"
			id="input_<%= query.id || 'none' %>"
			class="content-container text-black text-[3rem] outline-none font-bold w-full"
		></div>

		<div
			hx-get="components/tooltip"
			hx-swap="outerHTML"
			hx-trigger="load"
		></div>
		<div class="move-dropzone" data-position="right"></div>
		<div class="move-dropzone" data-position="left"></div>
		<div class="move-dropzone" data-position="below"></div>
		<div class="move-dropzone" data-position="top"></div>
	</div>
</div>
