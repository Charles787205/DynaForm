<div class="input-block" draggable="true" id="<%= query.id %>">
  <div class="block-container">
    <div class="flex input-flex py-2 gap-3">
      <div class="h-8 p-2 sample justify-start items-center gap-2.5 inline-flex">
        <div
          class="content-container border rounded-full h-4 w-4 border-gray-600"
          data-type="radiobox"
          data-name="radio_<%= query.id %>"
          class="w-4 h-4 text-blue-600 bg-gray-100 border-gray-300 focus:outline-none focus:ring-blue-500 dark:focus:ring-blue-600 dark:ring-offset-gray-800 focus:ring-2 dark:bg-gray-700 dark:border-gray-600"
        ></div>
        <div
          contentEditable="true"
          class="text-neutral-600 text-md w-fit font-extralight outline-none"
          >Default radio</div>
      <div hx-get="components/tooltip" hx-swap="outerHTML" hx-trigger="load"></div>
        
      </div>
      <button class="option" type="button" hx-get="/components/fields/radiobox" hx-trigger="click"
      hx-target="closest .input-block" hx-vals='{"placeholder": "Label", "id": "<%= query.id %>"}'
      hx-swap="beforebegin" onclick="handleClick(this)">Add option</button>
</div>
    <div class="move-dropzone" data-position="left"></div>
    <div class="move-dropzone" data-position="right"></div>
    <div class="move-dropzone" data-position="below"></div>
    <div class="move-dropzone" data-position="top"></div>
  </div>
</div>
